# Telegram Bot Token (получить у @BotFather)
TELEGRAM_BOT_TOKEN=your_bot_token_here

# OpenAI API Key (получить на platform.openai.com)
OPENAI_API_KEY=your_openai_key_here

# Database URL для PostgreSQL на Railway
# Формат: postgresql+asyncpg://user:password@host:port/dbname
DATABASE_URL=postgresql+asyncpg://postgres:password@localhost:5432/gym_bot

# Опциональные настройки
LOG_LEVEL=INFO
WHISPER_MODEL=whisper-1
GPT_MODEL=gpt-4o-mini
